<script type="text/javascript">
    $(function() {
        $('.btn').click(function() {            
			if($('#activity_enroll_username').val() == ''){
                alert('姓名不能为空');
                $('#activity_enroll_username').focus();
                return false;
            }else if($('#activity_enroll_contect').val() == ''){
                alert('联系电话不能为空');
                $('#activity_enroll_contect').focus();
                return false;
            }else if($('#activity_enroll_qq').val() == ''){
                alert('请输入班级');
                $('#activity_enroll_qq').focus();
                return false;
            }else if($('#activity_enroll_province_name').val() == ''){
                alert('姓名不能为空');
                $('#activity_enroll_province_name').focus();
                return false;
            }else if($('#activity_enroll_city_name').val() == ''){
                alert('姓名不能为空');
                $('#activity_enroll_city_name').focus();
                return false;
            }else if($('#activity_enroll_company_address').val() == ''){
                alert('姓名不能为空');
                $('#activity_enroll_company_address').focus();
                return false;
            }else if($('#activity_enroll_company_tel').val() == ''){
                alert('姓名不能为空');
                $('#activity_enroll_company_tel').focus();
                return false;
            }else if($('#activity_enroll_home_tel').val() == ''){
                alert('家庭电话不能为空');
                $('#activity_enroll_home_tel').focus();
                return false;
            }else if($('#activity_enroll_mobile').val() == ''){
                alert('手机号码不能为空');
                $('#activity_enroll_mobile').focus();
                return false;
            }else if($('#activity_enroll_mobile').val() && !test_mobile_number($('#activity_enroll_mobile').val()) ){
                alert('手机号码格式不正确');
                $('#activity_enroll_mobile').focus();
                return false;
            }else if($('#activity_enroll_email').val() == ''){
                alert('邮箱不能为空');
                $('#activity_enroll_email').focus();
                return false;
            }else if($('#activity_enroll_email').val() && !test_email_number($('#activity_enroll_email').val()) ){
                alert('邮箱格式不正确');
                $('#activity_enroll_email').focus();
                return false;
            }
        });
    });



    // 手机号码正则表达式
    function test_mobile_number(mobile_number) {
        if (mobile_number == undefined) {
            return false;
        }
        var mobile_reg = /^\d{11}$/;
        if (mobile_reg.test(mobile_number)){
            return true;
        };
        return false;
    }

    // 邮箱正则表达式
    function test_email_number(email_number) {
        if (email_number == undefined) {
            return false;
        }
        var email_reg = /^([^@\s]+)@((?:[-a-z0-9]+\.)+[a-z]{2,})$/i;
        if (email_reg.test(email_number)){
            return true;
        };
        return false;
    }
